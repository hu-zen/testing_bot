<launch>
    <node pkg="rosserial_python" type="serial_node.py" name="node_motor" output="screen">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="115200"/>
    </node>

    <node pkg="rosserial_python" type="serial_node.py" name="node_imu" output="screen">
        <param name="port" value="/dev/ttyUSB0"/>
        <param name="baud" value="57600"/>
    </node>

    <node pkg="rosserial_python" type="serial_node.py" name="node_encoder" output="screen">
        <param name="port" value="/dev/ttyUSB1"/>
        <param name="baud" value="115200"/>
    </node>
</launch>
